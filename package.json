{
  "author": "TSavo",
  "dependencies": {
    "@wopr-network/plugin-types": "^0.1.0",
    "winston": "^3.11.0"
  },
  "description": "Signal plugin for WOPR using signal-cli",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist/**/*",
    "IDENTITY.md"
  ],
  "keywords": [
    "wopr",
    "wopr-plugin",
    "signal",
    "signal-cli",
    "messaging"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@wopr-network/wopr-plugin-signal",
  "peerDependencies": {
    "wopr": "^2.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "check": "biome check src/ && tsc --noEmit",
    "format": "biome format --write src/",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "biome check --fix src/",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  }
}
